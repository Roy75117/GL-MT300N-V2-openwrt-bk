
config service 'ddns'
	option http_port '80'
	option https_port '443'
	option enabled_https '0'
	option enabled_ssh '1'
	option enabled_http '0'

config service 'glddns'
	option interface 'wan'
	option check_interval '10'
	option check_unit 'minutes'
	option force_interval '60'
	option force_unit 'minutes'
	option ip_url 'http://checkip.dyndns.com'
	option ip_source 'web'
	option password '3c177ff7b0a4a64a'
	option username '9483c4285adf'
	option domain 'jq85adf.glddns.com'
	option param_enc 'jq85adf'
	option lookup_host 'jq85adf.glddns.com'
	option service_name 'glddns.com'
	option force_ipversion '1'
	option enabled '1'

config service 'glddnsv6'
	option use_ipv6 '1'
	option interface 'wan6'
	option check_interval '10'
	option check_unit 'minutes'
	option force_interval '60'
	option force_unit 'minutes'
	option ip_url 'http://checkipv6.dyndns.com'
	option ip_source 'web'
	option password '3c177ff7b0a4a64a'
	option username '9483c4285adf'
	option domain 'jq85adf.glddns.com'
	option param_enc 'jq85adf'
	option lookup_host 'jq85adf.glddns.com'
	option service_name 'glddns.com'
	option force_ipversion '1'
	option enabled '1'

